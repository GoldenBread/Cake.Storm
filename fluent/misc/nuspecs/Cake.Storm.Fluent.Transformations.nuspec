<?xml version="1.0"?>
<package >
	<metadata>
		<id>{id}</id>
		<version>{version}</version>
		<title>Cake Fluent - Transformations</title>
		<authors>{author}</authors>
		<owners>Storm</owners>
		<license type="expression">MIT</license>
		<projectUrl>https://github.com/Julien-Mialon/Cake.Storm</projectUrl>
		<iconUrl>https://github.com/Julien-Mialon/Cake.Storm/blob/747b7d694868a37a1eedd2039f047c489903e92e/logo/icon.png</iconUrl>
		<requireLicenseAcceptance>false</requireLicenseAcceptance>
		<description>Cake addin for fluent description to update file content before build (csproj &amp; cs source file).</description>
		<releaseNotes>{release_notes}</releaseNotes>
		<copyright>Copyright 2017</copyright>
		<tags>cake cake-build cake-contrib fluent transformation edition csproj cs source</tags>
		<dependencies>
			<group>
				<dependency id="Cake.Common" version="{cake}" />
				<dependency id="Cake.Core" version="{cake}" />
				<dependency id="Cake.Storm.Fluent" version="{cake.storm.fluent}" />
				<dependency id="Microsoft.CodeAnalysis.CSharp" version="2.3.1" />
			</group>
		</dependencies>
	</metadata>
</package>